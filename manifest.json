{

    "manifest_version": 2,

    "name": "Proxy",
    "description": "Proxy on 127.0.0.1:8080",
    "version": "1.1",
    "background": {
    "scripts":["background.js"]
    },
    
  "permissions": [
      "tabs",
      "http://*/*",
      "https://*/*",
      "notifications",
      "contextMenus",
      "history",
      "background",
      "proxy",
      "webRequest",
      "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }